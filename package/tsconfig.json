{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "outDir": "../.moon/cache/types/package"
  },
  "include": [
    "src",
    "src/**/*.ts",
    "src/**/*.astro",
    "src/**/*.d.ts",
    "src/**/*.json",
    "virt-dev.d.ts",
  ],
}